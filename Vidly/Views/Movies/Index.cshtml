@model Vidly.ViewModels.MovieViewModel
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<table class="table table-striped table-bordered table-hover">
    <tr>
        <th>Id</th>
        <th>Name</th>
    </tr>
    @foreach (var movie in @Model.Movies)
    {
       
        <tr>

            <td>@movie.Id</td>
            <td>@movie.Name</td>
        </tr>
    }
</table>



